{
  "name": "@pixelworlds/opnsense-typescript-client",
  "version": "0.1.0",
  "description": "TypeScript client for OPNsense API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.1.25",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@eslint/js": "^8.57.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "globals": "^13.24.0",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
