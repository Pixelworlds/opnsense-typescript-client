    "version": "1.0.0"
  },
  "auth": {
    "type": "basic",
    "basic": [
      {
        "key": "username",
        "value": "{{opnsense_api_key}}",
        "type": "string"
      },
      {
        "key": "password",
        "value": "{{opnsense_api_secret}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "opnsense_base_url",
      "value": "https://10.0.1.1/api",
      "type": "string",
      "description": "OPNsense API base URL"
    },
    {
      "key": "api_key",
      "value": "your_api_key_here",
      "type": "string",
      "description": "OPNsense API key"
    },
    {
      "key": "api_secret",
      "value": "your_api_secret_here",
      "type": "string",
      "description": "OPNsense API secret"
    }
  ],
